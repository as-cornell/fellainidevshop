{#
/**
 * @file
 * Theme override to display a node.
 *
 */
#}


<article>

  {{ title_prefix }}
  {% if not page %}
    <h2{{ title_attributes }}>
      <a href="{{ url }}" rel="bookmark">{{ label }}</a>
    </h2>
  {% endif %}
  {{ title_suffix }}

  <div>
    node content here

    {{ content }}

  </div>


</article>
